<h1>Listing time_points</h1>

<table>
  <tr>
    <th>Time period</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @time_points.each do |time_point| %>
  <tr>
    <td><%= time_point.time_period %></td>
    <td><%= link_to 'Show', time_point %></td>
    <td><%= link_to 'Edit', edit_time_point_path(time_point) %></td>
    <td><%= link_to 'Destroy', time_point, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Time point', new_time_point_path %>
